<h1><%= @fund.name %></h1>

<% go = [] %>
<% @fund.months.each do |f| %>
	<% go << f unless f.id.nil? %>
<% end %>

<div style="padding:10px">
	<%= content_tag :div, "", id: "cumulative_return_chart", data: {months: go} %><br/>
	<%= link_to_unless_current 'AUM', @fund %>
	| <%= link_to_unless_current 'Gross/Net', show_gross_net_fund_path %>
	| <%= link_to_unless_current 'Return', show_cumulative_return_fund_path %>
</div>

<%= render 'show_rest' %>